{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/img/*",
      "/static/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/favicon.ico",
      "/robots.txt",
      "/sitemap.xml"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },

  "routes": [
    { "route": "/src/*", "statusCode": 410 }
  ]
}
